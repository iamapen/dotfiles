# alias for cygwin
alias open='cygstart'

# alias for cygwin-chef
alias chef-solo='/cygdrive/g/apps/opscode/chef/embedded/bin/ruby G:/apps/opscode/chef/bin/chef-solo'
alias knife='/cygdrive/g/apps/opscode/chef/embedded/bin/ruby G:/apps/opscode/chef/bin/knife'
alias chef-client='/cygdrive/g/apps/opscode/chef/embedded/bin/ruby G:/apps/opscode/chef/bin/chef-client'

# bash-completion
source ~/etc/bash_completion.d/vagrant
#source ~pen/etc/bash_completion.d/knife
